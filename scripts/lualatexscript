#! /bin/sh

lualatex -halt-on-error -synctex=1 -file-line-error -jobname=backup -gg $1 $2 $3 
echo $1 $2 $3  
myfile="${3%.*}"
mypdfstring=$myfile".pdf" 
mylogstring=$myfile".log" 
if ! grep -q Fatal ./build/backup.log; then
echo cool
myfile="${3%.*}"
mypdfstring=$myfile".pdf" 
mytexstring=$myfile".tex"
echo $mypdfstring
mv ./build/backup.pdf 
fi
