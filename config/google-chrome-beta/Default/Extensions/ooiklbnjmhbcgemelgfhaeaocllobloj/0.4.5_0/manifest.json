{
   "app": {
      "background": {
         "scripts": [ "background.js" ]
      }
   },
   "description": "Mosh (mobile shell) for Chrome",
   "icons": {
      "128": "laptop_terminal.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "42",
   "name": "Mosh",
   "offline_enabled": true,
   "permissions": [ {
      "socket": [ "resolve-host", "tcp-connect", "udp-bind", "udp-send-to" ]
   }, "clipboardRead", "clipboardWrite", "contextMenus", "notifications", "storage", "app.window.fullscreen.overrideEsc", "https://dns.google.com/resolve" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.4.5"
}
