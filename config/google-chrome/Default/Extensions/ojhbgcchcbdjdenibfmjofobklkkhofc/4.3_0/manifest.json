{
   "app": {
      "background": {
         "scripts": [ "EpubReader/scripts/background.js" ]
      }
   },
   "description": "Simple EPUB Reader for EPUB3 books.",
   "file_handlers": {
      "any": {
         "extensions": [ "epub" ]
      }
   },
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Simple EPUB Reader",
   "offline_enabled": true,
   "permissions": [ "storage", "contextMenus", "unlimitedStorage", "tts", {
      "fileSystem": [ "write", "retainEntries" ]
   } ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "4.3"
}
