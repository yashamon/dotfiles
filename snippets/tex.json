    "Plot 2D Graph": {
        "prefix": ["plotgraph2d", "\\plotgraph2d"],
        "body": [
            "\\addplot [",
            "\tdomain=${1:-10}:${2:10}," ,
            "\tsamples=70,", 
            "\tcolor=${3:blue},",
            "\t]",
            "\t{${4:x^2 + 2*x + 1}};",
            "\\addlegendentry{$${5:x^2 + 2x + 1}$}",
