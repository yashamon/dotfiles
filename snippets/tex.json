{
"G-bundle": {
    "prefix": "Gbun",
    "body": [
        "\\$$1\\$-Bundle $0" 
       ],
    "description": "dots"
    },
"three dots": {
    "prefix": ".",
    "body": [
        "\\ldots $0"
       ],
    "description": "dots"
    }, 
"super set": {
    "prefix": "sups",
    "body": [
        "\\supset $0"
       ],
    "description": "subset"
    }, 
"times": {
    "prefix": "t, times",
    "body": [
        "\\times $0"
       ],
    "description": "times"
    },  
"quotes": {
    "prefix": "\"",
    "body": [
        "``$1'' $0"
       ],
    "description": "quotes"
    }, 

  "text": {
    "prefix": "text",
    "body": [
        "\\text{$1} $0"
       ],
    "description": "text"
    }, 

"emphasize bold": {
    "prefix": "embf",
    "body": [
        "\\textbf{\\emph{$1}} $0"
       ],
    "description": "emphasize"
    }, 

"emphasize": {
    "prefix": "em",
    "body": [
        "\\emph{$1} $0"
       ],
    "description": "emphasize"
    }, 

"ni": {
    "prefix": "ni",
    "body": [
        "\\ni $0"
       ],
    "description": "subset"
    }, 


"subset": {
    "prefix": "sub",
    "body": [
        "\\subset $0"
       ],
    "description": "subset"
    }, 


  "Align(ed)": {
    "prefix": "ali",
    "body": [
        "\\begin{align}",
        "\t$0",    
        "\\end{align}"
],
    "description": "Align(ed)"
},
  "tikz": {
    "prefix": "tikz",
    "body": [
"\\begin{equation*}",
"\\begin{tikzcd}",
"$1 \\ar[r, \"$2\"] \\ar [d, \"$3\"] &  $4 \\ar [d,\"$5\"] \\\\", 
       "$6 \\ar [r, \"$7\"]   & $8",  
"\\end{tikzcd}",
"\\end{equation*}",
"$0"
  ],
    "description": "tikz"
  },


"Cases": {
    "prefix": "cas",
    "body": [
      "\\begin{cases}",
      "\t${1:equation}, &\\text{ if }${2:case}\\\\\\\\",
      "\t$0",
      "\\end{cases}"
    ],
    "description": "Cases"
  },
  "Chapter": {
    "prefix": "cha",
    "body": [
      "\\chapter{${1:chapter name}} % (fold)",
      "\\label{cha:${2:${1/(\\w+)(\\W+$)?|\\W+/${1:?${1:/asciify/downcase}:_}/g}}}",
      "${0:$TM_SELECTED_TEXT}",
      "% chapter $2 (end)"
    ],
    "description": "Chapter"
  },
  "Description": {
    "prefix": "desc",
    "body": [
      "\\\\begin{description}",
      "\t\\item[$1] $0",
      "\\\\end{description}"
    ],
    "description": "Description"
  },
  "Math": {
    "prefix": "m",
    "body": [
      "\\$$1\\$$0"
    ],
    "description": "Add a Math"
  },

"Equation": {
    "prefix": "eq",
    "body": [
      "\\begin{equation*}",
      "\t$1",
      "\\end{equation*}",
      "$0"
    ],
    "description": "Add a Equation"
  },
"EquationNumbered": {
    "prefix": "eqnum",
    "body": [
      "\\begin{equation}",
      "\t\\label{eq:$1}",
      "\t$2", 
      "\\end{equation}",
      "$0" 
      ],
    "description": "Add a Equation"
  }, 
 "proposition": {
    "prefix": "prop",
    "body": [
      "\\begin{proposition}", 
      "\t\\label{prop:$1}",   
      "$2",
      "\\end{proposition}",
      "$0"
      ],
    "description": "proposition"
  }, 
"remark": {
    "prefix": "rem",
    "body": [
      "\\begin{remark}", 
      "\t\\label{remark:$1}", 
      "$2", 
      "\\end{remark}",
      "$0"
   ],
    "description": "proposition"
  },
 "notation":{
    "prefix": "notation",
    "body": [
      "\\begin{notation}", 
      "\t\\label{notation:$1}",
      "\\end{notation}",
      "$0"
    ],
    "description": "Add notation"
  }, 
"example": {
    "prefix": "example",
    "body": [
      "\\begin{example}", 
      "\t\\label{example:$1}",
      "\\end{example}",
      "$0"
   ],
    "description": "Add a theorem"
  },

 "lemma": {
    "prefix": "lem",
    "body": [
      "\\begin{lemma}", 
      "\t\\label{lemma:$1}", 
      "$2",
      "\\end{lemma}",
      "$0"
   ],
    "description": "Add a theorem"
  },
 
 "Corollary": {
    "prefix": "corol",
    "body": [
      "\\begin{corol}", 
      "\t\\label{corol:$1}", 
      "$2",

      "\\end{corol}",
      "$0"
    ],
    "description": "Add a theorem"
  },

"Theorem": {
    "prefix": "thm",
    "body": [
      "\\begin{theorem}", 
      "\t\\label{thm:$1}", 
       "$2",

      "\\end{theorem}",
      "$0"
    ],
    "description": "Add a theorem"
  },
    "Proof": {
    "prefix": "proof",
    "body": [
      "\\begin{proof}",
      "\t$1",
      "\\end{proof}",
      "$0"
    ],
    "description": "Add a proof"
  },

}





